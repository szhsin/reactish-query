{
  "name": "reactish-query",
  "version": "0.8.1",
  "description": "Atomic state-powered, lightweight React query library",
  "author": "Zheng Song",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szhsin/reactish-query.git"
  },
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/",
    "types/",
    "composable/",
    "middleware/"
  ],
  "keywords": [
    "react",
    "query",
    "state",
    "data fetching"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.mjs"
    },
    "./composable": {
      "types": "./types/composable/index.d.ts",
      "require": "./dist/cjs/composable/index.cjs",
      "default": "./dist/esm/composable/index.mjs"
    },
    "./middleware": {
      "types": "./types/middleware/index.d.ts",
      "require": "./dist/cjs/middleware/index.cjs",
      "default": "./dist/esm/middleware/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "start": "run-p watch \"types -- --watch\"",
    "bundle": "rollup -c",
    "watch": "rollup -c -w",
    "types": "tsc",
    "clean": "rm -Rf dist types",
    "prepare": "rm -Rf types/__tests__",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "pret": "prettier -c .",
    "pret:fix": "prettier -w .",
    "build": "run-s pret clean types lint bundle",
    "test": "vitest run",
    "eg": "npm run dev --prefix examples"
  },
  "peerDependencies": {
    "react": "^16.14 || ^17 || ^18 || ^19"
  },
  "dependencies": {
    "reactish-state": "^2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.28.6",
    "@babel/preset-env": "^7.28.6",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.10",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@vitest/coverage-istanbul": "^4.0.18",
    "@vitest/coverage-v8": "^4.0.18",
    "@vitest/eslint-plugin": "^1.6.6",
    "babel-plugin-pure-annotations": "^0.1.2",
    "deplift": "^1.1.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-hooks-addons": "^0.5.0",
    "globals": "^17.1.0",
    "jsdom": "^27.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.8.1",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rollup": "^4.56.0",
    "rollup-plugin-add-directive": "^1.0.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.1",
    "vitest": "^4.0.18"
  }
}
